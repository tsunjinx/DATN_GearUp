{
  "name": "datn-gearup",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,vue,ts,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,ts,css,md}\"",
    "type-check": "vue-tsc --noEmit",
    "pre-commit": "npm run lint:check && npm run format:check",
    "prepare": "git config core.hooksPath .githooks || true"
  },
  "dependencies": {
    "animejs": "^4.1.2",
    "axios": "^1.11.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "vite": "^7.0.4",
    "vue-tsc": "^1.0.0"
  }
}
